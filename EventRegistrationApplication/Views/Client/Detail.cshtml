@{
    ViewBag.Title = "Detail";
}

<h2 style="text-align:center; margin-top: 20px; margin-bottom:20px">Client Details</h2>
<table class="table">
    <tr>
        <th>Email</th>
        <th>Name</th>
        <th>Address</th>
        <th>Age</th>
        <th>Phone no.</th>
    </tr>

    @foreach (var i in Model)
    {
        <tr>
            <td>@i.Email</td>
            <td>@i.FullName</td>
            <td>@i.Address</td>
            <td>@i.Age</td>
            <td>@i.Phone</td>

        </tr>
    }
</table>

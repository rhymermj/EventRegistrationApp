@{
    ViewBag.Title = "Search";
}

<h2 style="text-align:center; margin-top: 50px; margin-bottom:10px">Search Results</h2>
<table class="table">
    <tr>
        <th>Event Name</th>
        <th>Description</th>
        <th>Location</th>
        <th>Date</th>
        <th>Ticket Fee</th>
    </tr>

    @foreach (var i in Model)
    {
        <tr>
            <td>@i.EventName</td>
            <td>@i.Description</td>
            <td>@i.Location</td>
            <td>@i.Date</td>
            <td>@i.TicketFee</td>

        </tr>
    }
</table>
